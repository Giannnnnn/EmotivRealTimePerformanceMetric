<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PerformanceMetric.MainPage">

    <ScrollView>
        <VerticalStackLayout Padding="20">
            <Label Text="Performance Metrics" FontSize="15" FontAttributes="Bold" />
            <Label x:Name="InterestLabel" Text="Interest --" FontSize="30" />
            <Label x:Name="ExcitementLabel" Text="Excitement --" FontSize="30" />
            <Label x:Name="EngagementLabel" Text="Engagement --" FontSize="30" />
            <Label x:Name="RelaxationLabel" Text="Relaxation --" FontSize="30" />
            <Label x:Name="AttentionLabel" Text="Attention --" FontSize="30" />
            <Label x:Name="StressLabel" Text="Stress --" FontSize="30" />
        </VerticalStackLayout>
    </ScrollView>

</ContentPage>
